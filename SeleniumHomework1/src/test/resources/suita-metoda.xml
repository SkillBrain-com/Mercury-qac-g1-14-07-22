<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Prima suita de TestNg" verbose="1" >
    <test name="Primul test din TestNg .xml file" >
        <classes>
            <class name="TestNgAnnotations">
                <methods>
                    <exclude name="test1"></exclude>
                </methods>
            </class>
        </classes>

    </test>

    <test name="Al doilea test din TestNg .xml file">
        <classes>
            <class name="TestNgSeleniumTests">
                <methods>
                    <include name="test1"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>